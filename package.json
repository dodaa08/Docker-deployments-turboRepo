{
  "name": "test",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "db:generate" : "cd packages/db && npx prisma generate && cd ../..",
    "db:push": "cd packages/db && bunx prisma db push && cd ../..",
    "start:web" : "cd apps/web && bun run start",
    "start:backend" : "cd apps/backend && bun run dev",
    "start:ws" : "cd apps/websockets && bun run dev"
  },
  "devDependencies": {
    "prettier": "^3.7.4",
    "turbo": "^2.7.3",
    "typescript": "5.9.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "bun@1.3.3",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@types/express": "^5.0.6",
    "express": "^5.2.1"
  }
}
